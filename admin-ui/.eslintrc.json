{
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "import/no-unresolved": "off",
    "import/named": "off",
    "no-console": "warn",
    "react-hooks/exhaustive-deps": "warn"
  },
  "overrides": [
    {
      "files": ["src/lib/**/*.ts", "src/lib/**/*.tsx"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
