# AI Documentation Cache System Configuration
# PRD-001: HTTP API Foundation - Environment Configuration

# API Configuration
API_TITLE="AI Documentation Cache System API"
API_VERSION="1.0.0"
API_DESCRIPTION="Intelligent documentation cache with AI-powered search and enrichment"

# Server Configuration
HOST="0.0.0.0"
PORT=8080
RELOAD=true
LOG_LEVEL="info"

# CORS Configuration
CORS_ORIGINS=["*"]
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=["GET", "POST", "PUT", "DELETE", "OPTIONS"]
CORS_ALLOW_HEADERS=["*"]

# Security Configuration
SECURITY_HEADERS_ENABLED=true

# Environment
ENVIRONMENT="development"
DEBUG=true