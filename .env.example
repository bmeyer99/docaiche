# AI Documentation Cache System - Portainer Deployment Example

# --- API Service ---
APP_ENV=production
APP_PORT=8000
LOG_LEVEL=info
DATABASE_URL=sqlite:////data/app.db
REDIS_URL=redis://redis:6379/0
ANYTHINGLLM_URL=http://anythingllm:3001
OLLAMA_URL=http://ollama:11434
API_AUTH_SECRET=changeme_api_auth_secret
API_JWT_SECRET=changeme_api_jwt_secret

# --- Web UI Service ---
WEBUI_SESSION_SECRET=changeme_webui_session_secret
API_URL=http://api:8000

# --- AnythingLLM Service ---
VECTOR_DB_PATH=/data/vector_db
ANYTHINGLLM_API_KEY=changeme_anythingllm_api_key
ANYTHINGLLM_WORKSPACE_SECRET=changeme_anythingllm_workspace_secret

# --- Ollama Service ---
OLLAMA_MODEL=llama3
OLLAMA_MODEL_PATH=/models/llama3.bin
OLLAMA_API_KEY=changeme_ollama_api_key
OLLAMA_MODEL_SECRET=changeme_ollama_model_secret

# --- General ---
ENVIRONMENT=production
DEBUG=false