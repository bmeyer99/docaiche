      "id": 1,
      "options": {
        "cellHeight": "md",
        "footer": {
          "countRows": false,
          "fields": "",
          "reducer": [
            "sum"
          ],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": false,
            "displayName": "Service"
          }
        ]
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "up",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "time() - process_start_time_seconds",
          "format": "table",
          "instant": true,
          "refId": "B"
        }
      ],
      "title": "Service Health Status",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true
            },
            "indexByName": {},
            "renameByName": {
              "Value #A": "Status",
              "Value #B": "Uptime_Seconds",
              "job": "Service"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Uptime_Display",
            "mode": "stringReplace",
            "reduce": {
              "reducer": "lastNotNull"
            },
            "binary": {
              "left": "Uptime_Seconds",
              "operator": "/",
              "right": "3600"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Service",
            "mode": "string",
            "reduce": {
              "reducer": "last"
            },
            "replaceFields": true,
            "binary": {
              "left": "Service",
              "reducer": "sum",
              "right": ""
            },
            "replacePattern": "^node$",
            "replaceValue": "Node Exporter"
          }
        },
        {
          "id": "sortBy",
          "options": {
            "fields": {},
            "sort": [
              {
                "field": "Service"
              }
            ]
          }
        }
      ],
      "type": "table"
    },
