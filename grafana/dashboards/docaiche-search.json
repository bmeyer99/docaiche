      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "expr": "{job=\"docaiche_api\"} |= \"Search:\" | regexp \"Search: (?P<query>.+)\" | line_format \"{{.query}}\"",
          "refId": "A"
        }
      ],
